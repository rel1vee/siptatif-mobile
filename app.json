{
  "expo": {
    "name": "SIPTATIF - UIN Suska Riau",
    "slug": "siptatif-uin-suska",
    "version": "1.0.0",
    "description": "Sistem Informasi Pendaftaran Tugas Akhir Teknik Informatika",
    "orientation": "portrait",
    "icon": "./assets/uin-icon.png",
    "scheme": "siptatif",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mzes.siptatifuinsuska"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/uin-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mzes.siptatifuinsuska"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/uin-icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "057d2b23-9332-4eab-a489-64fc0fd55e39"
      }
    }
  }
}
